pipeline {
    agent any
    stages {
        stage('Example') {
            when {
                anyOf {
                    branch "main"
                }
            }
            steps {
                echo 'Hi Mitun, this is Master'
            }
        }
        stage('Example2') {
            when {
                anyOf {
                    branch "QA"
                }
            }
            steps {
                echo 'Hi Mitun, this is QA'
            }
        }
        stage('Example3') {
            when {
                anyOf {
                    branch "development"
                }
            }
            steps {
                 echo 'Hi Mitun, this is development'
            }
        }
    }
}
