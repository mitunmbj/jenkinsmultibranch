pipeline {
    agent any
    stages {
        stage('Example') {
            when {
                anyOf {
                    branch "main"
                }
            }
            agent { label 'docker_linux' }
            steps {
                sh "echo 'Hi Mitun, this is Master'"
            }
        }
        stage('Example2') {
            when {
                anyOf {
                    branch "QA"
                }
            }
            agent { label 'docker_linux' }
            steps {
                sh "echo 'Hi Mitun, this is QA'"
            }
        }
        stage('Example2') {
            when {
                anyOf {
                    branch "development"
                }
            }
            agent { label 'docker_linux' }
            steps {
                sh "echo 'Hi Mitun, this is development'"
            }
        }
    }
}
